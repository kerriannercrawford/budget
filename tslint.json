{
  "defaultSeverity": "error",
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
      "trailing-comma": true,
      "no-console": false,
      "no-async-without-await": true,
      "no-empty": true,
      "interface-name": true,
      "newline-before-return": true,
      "no-trailing-whitespace": true,
      "semicolon": true,
      "no-var-requires": false
  },
  "rulesDirectory": []
}